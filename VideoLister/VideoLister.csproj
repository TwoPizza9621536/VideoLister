<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0;net5.0;netstandard2.1;netstandard2.0;netcoreapp3.1;netcoreapp2.1;net48;net47;net462</TargetFrameworks>
		<Authors>Samuel Wu</Authors>
		<PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/Twopizza9621536/LikedVideoLister-dotnet.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageProjectUrl>https://github.com/Twopizza9621536/LikedVideoLister-dotnet</PackageProjectUrl>
		<PackageTags>YouTube</PackageTags>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<NeutralLanguage>en-US</NeutralLanguage>
		<Copyright>Copyright (c) 2021, Samuel Wu</Copyright>
		<Description>A lister for liked videos on YouTube.</Description>
		<PackageReleaseNotes>Now allowing the user use list any vidoes from a playlist.</PackageReleaseNotes>
		<Version>3.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md" Pack="True" PackagePath="/" />
		<None Include="..\LICENSE" Pack="True" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Google.Apis.YouTube.v3" Version="1.55.0.2449" />
	</ItemGroup>

</Project>
